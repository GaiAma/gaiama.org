    "gatsby-remark-emoji": "^0.0.1",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-responsive-iframe": "^1.4.17",
    "gatsby-remark-smartypants": "^1.4.11",
    "gatsby-transformer-javascript-frontmatter": "^1.0.2",
    "gatsby-transformer-json": "^1.0.14",
    "gatsby-transformer-remark": "^1.7.33",
    "gatsby-transformer-yaml": "^1.5.14",
    "jest": "^22.4.3",
    "jest-cli": "^22.4.3",
    "jest-image-snapshot": "^2.4.0",
    "make-dir": "^1.2.0",
    "netlify-cli": "^1.2.2",
    "prettier": "^1.11.1",
    "puppeteer": "^1.3.0",
