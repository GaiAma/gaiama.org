{
  "name": "gaiama.org",
  "private": true,
  "description": "GaiAma.org website",
  "version": "2.0.0-beta.4",
  "author": "Can Rau <cansrau@gmail.com>",
  "license": "MIT",
  "homepage": "https://www.gaiama.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaiama/gaiama.org.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "gatsby"
  ],
  "bugs": {
    "url": "https://github.com/gaiama/gaiama.org/issues"
  },
  "scripts": {
    "dev": "gatsby develop -S",
    "dev:full": "GAIAMA_FULL_CONTENT=1 yarn dev",
    "lint": "eslint --ext .js,.jsx .",
    "fix": "eslint --fix --ext .js,.jsx .",
    "test": "jest",
    "format": "prettier --config .prettierrc --write 'src/**/*.js' 'plugins/**/src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "clean:cache": "rimraf .cache",
    "clean:public": "find ${PWD}/public/* -not -path '*public/static*' -exec rm -rf {} +",
    "clean:public:all": "rimraf public",
    "clean": "yarn clean:cache && yarn clean:public",
    "clean:all": "yarn clean:cache && yarn clean:public:all",
    "build": "GAIAMA_FULL_CONTENT=1 gatsby build",
    "serve": "serve",
    "preview": "yarn build && yarn serve",
    "precommit": "yarn format",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -u",
    "github-release": "conventional-github-releaser -p angular -r 0",
    "deploy": "netlify deploy",
    "setup:git": "git config commit.template .github/commit_template"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "Opera 12.1"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-brands-svg-icons": "^5.2.0",
    "@fortawesome/free-regular-svg-icons": "^5.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@gaiama/query-string": "^0.1.0",
    "archieml": "^0.4.2",
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "conventional-changelog-cli": "^2.0.5",
    "conventional-github-releaser": "^3.1.2",
    "cryptocurrency-icons": "https://github.com/atomiclabs/cryptocurrency-icons",
    "cuid": "^2.1.3",
    "date-fns": "^1.29.0",
    "emotion": "^9.2.10",
    "emotion-server": "^9.2.10",
    "feed": "^2.0.1",
    "gatsby": "^2.0.7",
    "gatsby-image": "^2.0.8",
    "gatsby-plugin-canonical-urls": "^2.0.5",
    "gatsby-plugin-catch-links": "^2.0.2",
    "gatsby-plugin-emotion": "^2.0.5",
    "gatsby-plugin-layout": "^1.0.1",
    "gatsby-plugin-manifest": "^2.0.2",
    "gatsby-plugin-netlify": "^2.0.0",
    "gatsby-plugin-netlify-cache": "^0.1.0",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sharp": "^2.0.5",
    "gatsby-plugin-typography": "^2.2.0",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.1",
    "gatsby-source-instagram": "^0.1.3",
    "gatsby-transformer-sharp": "^2.1.1",
    "gray-percentage": "^2.0.0",
    "hex2rgba": "^0.0.1",
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.11",
    "lozad": "^1.3.0",
    "moment-timezone": "^0.5.14",
    "prop-styles": "^2.0.0",
    "prop-types": "^15.6.0",
    "qr-image": "^3.2.0",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-emotion": "^9.2.10",
    "react-head": "^3.0.0",
    "react-headroom": "^2.2.4",
    "react-helmet": "^5.2.0",
    "react-media": "^1.8.0",
    "react-textarea-autosize": "^7.0.4",
    "react-toastify": "^4.3.1",
    "react-typography": "1.0.0-alpha.4",
    "rehype-react": "^3.0.3",
    "slash": "^2.0.0",
    "slugify": "^1.2.9",
    "speakingurl": "^14.0.1",
    "typography": "1.0.0-alpha.4",
    "unist-util-modify-children": "^1.1.2",
    "unist-util-select": "^1.5.0",
    "validator": "^10.7.1"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "dotenv": "^6.0.0",
    "eslint": "^5.5.0",
    "eslint-config-google": "^0.10.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.80.0",
    "gatsby-remark-autolink-headers": "^2.0.0-rc.1",
    "gatsby-remark-custom-blocks": "^2.0.0-rc.1",
    "gatsby-remark-embed-video": "^1.4.0",
    "gatsby-remark-emoji": "^0.0.2",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-responsive-iframe": "^2.0.0-rc.1",
    "gatsby-remark-smartypants": "^2.0.0-rc.1",
    "gatsby-transformer-javascript-frontmatter": "^2.0.0-rc.2",
    "gatsby-transformer-json": "^2.1.1-rc.1",
    "gatsby-transformer-remark": "^2.1.1-rc.1",
    "gatsby-transformer-yaml": "^2.1.1-rc.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-image-snapshot": "^2.5.0",
    "make-dir": "^1.3.0",
    "netlify-cli": "^1.2.2",
    "prettier": "^1.11.1",
    "remark-iframes": "^3.0.1",
    "serve": "^10.0.0"
  }
}
