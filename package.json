    "gatsby-remark-images-with-ratio": "https://gitlab.com/GaiAma/gatsby-remark-images-with-ratio.git",
    "gatsby-remark-wrap-images": "https://gitlab.com/GaiAma/gatsby-remark-wrap-images.git",
    "gatsby-source-filesystem": "https://gitlab.com/GaiAma/gatsby-source-filesystem.git",
    "gatsby-transformer-all-yaml": "https://gitlab.com/GaiAma/gatsby-transformer-all-yaml.git",
    "gatsby-transformer-archieml": "https://gitlab.com/GaiAma/gatsby-transformer-archieml.git",
    "gatsby-transformer-qr": "https://gitlab.com/GaiAma/gatsby-transformer-qr.git",
    "gatsby-transformer-sharp": "https://gitlab.com/GaiAma/gatsby-transformer-sharp.git",
    "glamor": "^2.20.40",
    "glamorous": "^4.11.4",
    "hex2rgba": "^0.0.1",
    "lodash": "^4.17.5",
    "lozad": "^1.3.0",
    "moment-timezone": "^0.5.14",
    "prop-styles": "^2.0.0",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react-headroom": "^2.2.2",
    "react-helmet": "^5.2.0",
    "react-media": "^1.8.0",
