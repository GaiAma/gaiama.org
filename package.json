{
  "name": "gaiama.org",
  "description": "GaiAma.org website",
  "version": "1.5.32",
  "author": "Can Rau <cansrau@gmail.com>",
  "license": "MIT",
  "homepage": "https://www.gaiama.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaiama/gaiama.org.git"
  },
  "keywords": [
    "gatsby"
  ],
  "bugs": {
    "url": "https://github.com/gaiama/gaiama.org/issues"
  },
  "scripts": {
    "dev": "gatsby develop",
    "lint": "eslint --ext .js,.jsx .",
    "fix": "eslint --fix --ext .js,.jsx .",
    "size": "size-limit",
    "test": "jest",
    "format": "prettier --config .prettierrc --write 'src/**/*.js' 'plugins/**/src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "clean:cache": "rimraf .cache",
    "clean:public": "rimraf public",
    "clean": "yarn clean:cache && yarn clean:public",
    "build": "NODE_ENV=production gatsby build",
    "serve": "serve",
    "preview": "yarn build && yarn serve",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "precommit": "yarn format",
    "deploy": "netlify deploy"
  },
  "browserslist": [
    "> 1%",
    "last 4 versions",
    "Firefox ESR",
    "Opera 12.1"
  ],
  "size-limit": [
    {
      "path": "public/*.js",
      "webpack": false
    }
  ],
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.4",
