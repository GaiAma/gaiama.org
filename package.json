    "gray-percentage": "2.0.0",
    "hex2rgba": "0.0.1",
    "js-yaml": "3.12.0",
    "lodash": "4.17.11",
    "moment-timezone": "0.5.23",
    "prop-styles": "2.0.0",
    "prop-types": "15.6.2",
    "qr-image": "3.2.0",
    "ramda": "0.26.1",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-emotion": "9.2.12",
    "react-head": "3.0.2",
