    "flow-bin": "^0.68.0",
    "gatsby-cli": "^1.1.58",
    "gatsby-node-helpers": "^0.1.3",
    "gatsby-plugin-webpack-bundle-analyzer": "^0.1.1",
    "gatsby-remark-autolink-headers": "^1.4.13",
