    "gatsby-transformer-remark": "2.1.15",
    "gatsby-transformer-sharp": "2.1.9",
    "gatsby-transformer-yaml": "2.1.6",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-image-snapshot": "2.7.0",
    "make-dir": "1.3.0",
    "prettier": "1.15.3",
    "remark-iframes": "3.0.6",
