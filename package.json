{
  "name": "gaiama.org",
  "private": true,
  "description": "GaiAma.org website",
  "version": "2.5.3",
  "author": "Can Rau <cansrau@gmail.com>",
  "license": "MIT",
  "homepage": "https://www.gaiama.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaiama/gaiama.org.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=10.10"
  },
  "bugs": {
    "url": "https://github.com/gaiama/gaiama.org/issues"
  },
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 GATSBY_TELEMETRY_DISABLED=1 GATSBY_TELEMETRY_DEBUG=0 GATSBY_GRAPHQL_IDE=playground ENABLE_GATSBY_REFRESH_ENDPOINT=1 gatsby develop -So",
    "dev:full": "GATSBY_GRAPHQL_IDE=playground GAIAMA_FULL_CONTENT=1 yarn dev",
    "debug": "DEBUG=* yarn dev",
    "debug:gatsby": "DEBUG=gatsby* yarn dev",
    "test": "jest",
    "clean:cache": "find ${PWD}/.cache/* -not -path '*.cache/@gaiama*' -exec rm -rf {} +",
    "clean:cache:all": "rimraf .cache",
    "clean:public": "find ${PWD}/public/* -not -path '*public/static*' -exec rm -rf {} +",
    "clean:public:all": "rimraf public",
    "clean": "yarn clean:cache && yarn clean:public",
    "clean:all": "yarn clean:cache:all && yarn clean:public:all",
    "build": "GAIAMA_FULL_CONTENT=1 gatsby build",
    "serve": "serve -c public/serve.json",
    "preview": "yarn clean && yarn build && yarn serve",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 -u",
    "github-release": "conventional-github-releaser -p angular -r 0",
    "setup:git": "git config commit.template .github/commit_template",
    "contributors:add": "all-contributors add",
    "r": "curl -X POST https://localhost:8000/__refresh",
    "contributors:generate": "all-contributors generate"
  },
  "dependencies": {
